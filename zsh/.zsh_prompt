ZSH_THEME="robbyrussell"
[ -d $HOME/.oh-my-zsh ] && export ZSH="$HOME/.oh-my-zsh"
[ -d $HOME/.oh-my-zsh ] && . $ZSH/oh-my-zsh.sh
[ -d $HOME/.oh-my-zsh ] || autoload -U colors && colors
[ -d $HOME/.oh-my-zsh ] || PS1="%B%{$fg[red]%}[%{$fg[yellow]%}%n%{$fg[green]%}@%{$fg[blue]%}%M %{$fg[magenta]%}%~%{$fg[red]%}]%{$reset_color%}$%b "
